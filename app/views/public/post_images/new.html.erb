<h2>新規投稿</h2>
<%= form_with model:@post_image, url: post_images_path, method: :post do |f| %>

<%= f.label :title,"タイトル" %>
<%= f.text_field :title %>
<%= f.label :introduction,"説明文" %>
<%= f.text_area :introduction %>
<%= f.label :image,"画像" %>
<%= f.file_field :image, accept: "image/*" %>

<%= f.submit "投稿" %>
<% end %>